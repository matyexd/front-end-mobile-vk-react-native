{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@hooks/*": [
				"./src/hooks/*"
			],
			"@assets/*": [
				"./src/assets/*"
			],
			"@ui-kit/*": [
				"./src/components/ui-kit/*"
			],
			"@utils/*": [
				"./src/utils/*"
			],
			"@components/*": [
				"./src/components/*"
			],
			"@type/*": [
				"./src/store/types/*"
			],
		}
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}